{
  "name": "coin-purse",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "inject-env": "node ./scripts/inject-env.js",
    "android": "npm run inject-env && expo run:android",
    "ios": "npm run inject-env && expo run:ios",
    "web": "expo start --web",
    "android:dev": "ENV=dev npm run inject-env && expo run:android --variant=devDebug",
    "android:local": "ENV=local npm run inject-env && expo run:android --variant=localDebug",
    "android:prod": "ENV=prod npm run inject-env && expo run:android --variant=prodDebug",
    "ios:dev": "ENV=dev npm run inject-env && expo run:ios --scheme dev",
    "ios:local": "ENV=local npm run inject-env && expo run:ios --scheme local",
    "ios:prod": "ENV=prod npm run inject-env && expo run:ios --scheme prod",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build:android:prod:apk": "cd android && ENVFILE=.env.prod ./gradlew assembleProdRelease",
    "build:android:prod:aab": "cd android && ENVFILE=.env.prod ./gradlew bundleProdRelease"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@revopush/react-native-code-push": "^1.2.0",
    "date-fns": "^4.1.0",
    "expo": "~52.0.43",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.7",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "lottie-react-native": "7.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-config": "^1.5.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@react-native-community/cli": "^18.0.0",
    "@react-native/metro-config": "^0.80.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
